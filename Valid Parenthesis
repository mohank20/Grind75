class Solution {
public:
    bool isValid(string A) {
        stack<char> s;
        for(auto x : A){
            if(x=='('||x=='{'||x=='[') s.push(x);
            else if (s.empty()||(x==')'&& s.top()!='(')
            ||(x==']'&&s.top()!='[')||(x=='}'&&s.top()!='{')) return false;
            else s.pop();
        }    
        return s.empty();
    }
};
